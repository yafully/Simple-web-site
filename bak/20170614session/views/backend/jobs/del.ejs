<%-  include('../header.ejs') %>
    <h1>删除学生信息</h1>
    <form method="post">
        <ul>
            <% for(var i = 0; i < result.length; i++) { %>
            <li>
                <label for="job">
                职位：<%= result[i].jobName %>，部门：<%= result[i].depart %>，详情：<%= result[i].detail %>，发布时间：<%= result[i].date %>
                </label>
                <input type="radio" name="job" value=<%= result[i]._id%>>
            </li>
            <% } %>
        </ul>
        <button type="submit">确定</button>
    </form>
<%-  include('../footer.ejs') %>