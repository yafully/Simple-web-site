	<!--@MenuCall-->
	<div class="menuBar">
		<svg class="icon_menu menuCall" viewBox="0 0 100 100">
			<polyline class="line one" points="10 20, 90 20" />
			<polyline class="line two" points="10 50, 90 50" />
			<polyline class="line three" points="10 80, 90 80" />
